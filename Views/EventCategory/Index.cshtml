@using Ch26_Practice_2___Razor_Forms.Models

@model List<EventCategory>

<h1>Coding Events</h1>



@if (Model.Count == 0)
{
    <p>No events to categorize yet!</p>
}
else
{
    <table class="table">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {

                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                </tr>
            }
        </tbody>
    </table>
}


